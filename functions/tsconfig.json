{
  "extends": "../paths.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "jsx": "preserve",
    "noEmitOnError": false
  },
  "compileOnSave": true,
  "include": [
    "src"
  ]
}
